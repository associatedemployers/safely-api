{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Launch API in inspection mode",
    "runtimeExecutable": "yarn",
    "runtimeArgs": ["run", "debug"],
    "envFile": "${workspaceFolder}/.env",
    "port": 9229,
    "autoAttachChildProcesses": true,
    "skipFiles": [
      "<node_internals>/**"
    ]
  }]
}